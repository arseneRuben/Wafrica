{
    "extends": [],
    "rules": {
        "indent": [
            "error",
            2
        ],
        "react/jsx-indent": [
            "error",
            2
        ],
        "react/jsx-indent-props": [
            "error",
            2
        ],
        "react/prop-types": 0
    },
    "overrides": [
        {
            "files": [
                "**/*.js",
                "**/*.jsx"
            ]
        }
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }
}